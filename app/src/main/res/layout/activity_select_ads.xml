<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout555"
        android:layout_width="431dp"
        android:layout_height="624dp"
        android:background="#FFFFFF"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

        <TextView
            android:id="@+id/titleJob"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:fontFamily="sans-serif-black"
            android:text=" TextView"
            android:textColor="#000000"
            android:textSize="24sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:fontFamily="sans-serif-medium"
            android:text="    Description :"
            android:textColor="#070707"
            android:textSize="16sp" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="114dp"
            android:background="@drawable/button_transparent_borderblack">

            <TextView
                android:id="@+id/selDes"
                android:layout_width="422dp"
                android:layout_height="108dp"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:fontFamily="sans-serif-medium"
                android:text="TextView"
                android:textColor="#000000"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="170dp"
            android:background="@drawable/button_transparent_borderblack">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="34dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/linkurl"
                        android:layout_width="154dp"
                        android:layout_height="34dp"
                        android:fontFamily="sans-serif-medium"
                        android:text="    Salary  :"
                        android:textColor="#070707"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/selLink"
                        android:layout_width="280dp"
                        android:layout_height="match_parent"
                        android:fontFamily="sans-serif-black"
                        android:text="  TextView"
                        android:textAllCaps="false"
                        android:textColor="#000000"
                        android:textSize="18sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView20"
                        android:layout_width="208dp"
                        android:layout_height="34dp"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:text="    Salary Period  :"
                        android:textColor="#070707"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/selPeriod1"
                        android:layout_width="330dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-black"
                        android:text="  TextView"
                        android:textAllCaps="false"
                        android:textColor="#000000"
                        android:textSize="18sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView21"
                        android:layout_width="210dp"
                        android:layout_height="34dp"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:text="    Position Type  :"
                        android:textColor="#070707"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/selPos1"
                        android:layout_width="330dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-black"
                        android:text="  TextView"
                        android:textAllCaps="false"
                        android:textColor="#000000"
                        android:textSize="18sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="42dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView22"
                        android:layout_width="204dp"
                        android:layout_height="34dp"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-medium"
                        android:text="    Phone  :"
                        android:textColor="#070707"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/selPhone1"
                        android:layout_width="330dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:fontFamily="sans-serif-black"
                        android:text="  TextView"
                        android:textAllCaps="false"
                        android:textColor="#000000"
                        android:textSize="18sp" />
                </LinearLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:background="@drawable/button_transparent_borderblack"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView30"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:fontFamily="sans-serif-medium"
                android:text="    Address  :"
                android:textColor="#070707"
                android:textSize="18sp" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="50dp">

                <TextView
                    android:id="@+id/selLoc"
                    android:layout_width="423dp"
                    android:layout_height="46dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="sans-serif-medium"
                    android:text=" TextView"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="18sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:background="@drawable/button_transparent_borderblack"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView937"
                    android:layout_width="154dp"
                    android:layout_height="34dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="    Link/ URL  :"
                    android:textColor="#070707"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/selUrl"
                    android:layout_width="280dp"
                    android:layout_height="match_parent"
                    android:fontFamily="sans-serif-black"
                    android:autoLink="email|web"
                    android:text="  TextView"
                    android:textAllCaps="false"
                    android:textColor="#000000"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView23"
                    android:layout_width="154dp"
                    android:layout_height="34dp"
                    android:layout_weight="1"

                    android:fontFamily="sans-serif-medium"
                    android:text="   Email  :"
                    android:textColor="#050505"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/selMail"
                    android:layout_width="280dp"
                    android:layout_height="34dp"
                    android:layout_weight="1"
                    android:autoLink="email|web"
                    android:fontFamily="sans-serif-black"
                    android:textColor="#050505"
                    android:textSize="18sp" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="437dp"
        android:layout_height="62dp"
        android:background="@drawable/gradient_background"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:id="@+id/selChat"
            android:layout_width="104dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Chat"
            android:textSize="18sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/selCall"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="Call"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="bold" />
    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="470dp"
        android:layout_height="136dp"
        android:background="@drawable/gradient_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView320"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginStart="29dp"
            android:layout_marginLeft="29dp"
            android:layout_marginEnd="297dp"
            android:layout_marginRight="297dp"
            android:layout_marginBottom="22dp"
            android:background="@android:color/transparent"
            android:visibility="visible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@mipmap/ic_launcher_round" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>